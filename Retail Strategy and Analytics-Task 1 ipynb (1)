   "outputs": [],
   "source": [
    "# Convert the integer format to a date format\n",
    "# A search online shows that CSV and Excel integer dates begin on 30 Dec 1899\n",
    "transaction_data['DATE'] = pd.to_datetime(transaction_data['DATE'], origin='1899-12-30', unit='D')"
   ]
  },
