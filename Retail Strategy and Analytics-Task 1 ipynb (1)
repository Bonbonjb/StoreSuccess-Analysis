   "outputs": [],
   "source": [
    "# Calculate total sales and total quantity by segment\n",
    "avg_price_per_segment = QVI_data.groupby(['LIFESTAGE', 'PREMIUM_CUSTOMER']).apply(lambda x: (x['TOT_SALES'].sum() / x['PROD_QTY'].sum())).reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
   "id": "de4806c2-c22d-488a-8a3e-adecc2d74e54",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
