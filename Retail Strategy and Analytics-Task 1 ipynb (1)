   "source": [
    "plt.figure(figsize=(12, 6))\n",
    "\n",
    "# Create a bar plot\n",
    "sns.barplot(data=avg_units, x='LIFESTAGE', y='AVG', hue='PREMIUM_CUSTOMER', dodge=True, palette='Greens_r')\n",
    "\n",
    "# Add labels and title\n",
    "plt.xlabel('Lifestage')\n",
    "plt.ylabel('Avg Units per Transaction')\n",
    "plt.title('Units per Customer by Lifestage and Premium Customer Flag')\n",
    "\n",
    "# Rotate x-axis labels for better readability\n",
    "plt.xticks(rotation=90)\n",
