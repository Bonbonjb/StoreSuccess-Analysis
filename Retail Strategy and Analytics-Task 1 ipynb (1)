   "cell_type": "code",
   "execution_count": 69,
   "id": "d945dc40-bd33-478a-b2a9-3caea1936c81",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate total number of customers for proportions\n",
    "total_customers = customer_counts['LYLTY_CARD_NBR'].sum()\n",
    "\n",
    "# Calculate proportions\n",
    "customer_counts['Proportion'] = customer_counts['LYLTY_CARD_NBR'] / total_customers\n",
    "\n",
    "# Convert proportions to percentages\n",
    "customer_counts['Percentage'] = customer_counts['Proportion'] * 100"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 117,
   "id": "386f8473-7952-4ffa-afb8-5f76d61c423d",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x600 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Pivot the data for heatmap\n",
    "heatmap_data = customer_counts.pivot_table(index='LIFESTAGE', columns='PREMIUM_CUSTOMER', values='Percentage')\n",
    "\n",
    "# Create the heatmap\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.heatmap(heatmap_data, annot=True, fmt='.1f', cmap='coolwarm', cbar=True, linewidths=.5, annot_kws={\"size\": 10, \"weight\": \"bold\"})\n",
    "\n",
    "# Add labels and title\n",
    "plt.xlabel('Premium Customer Flag')\n",
    "plt.ylabel('Lifestage')\n",
    "plt.title('Proportion of Customers by Lifestage and Premium Customer Flag')\n",
    "\n",
    "plt.show()"
   ]
  },
