   "source": [
    "# Rename the columns for clarity\n",
    "total_sales.columns = ['LIFESTAGE', 'PREMIUM_CUSTOMER', 'TOT_SALES']\n",
    "\n",
    "# Display the results\n",
    "total_sales"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "id": "c2cb363a-85e6-4446-baf2-e5d0530261c6",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create a pivot table\n",
    "pivot = total_sales.pivot_table(values='TOT_SALES', index='PREMIUM_CUSTOMER', columns='LIFESTAGE', aggfunc='sum', fill_value=0)\n",
    "\n",
    "# Normalize the sales by column to get the proportion\n",
    "proportion = pivot.div(pivot.sum(axis=0), axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 124,
   "id": "cee3031b-8500-461e-a269-2662df87b608",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Plotting\n",
    "proportion.T.plot(kind='bar', stacked=True, figsize=(12, 6), color=['seagreen', 'mediumseagreen', 'springgreen'])\n",
    "\n",
    "# Adding labels\n",
    "for i in range(proportion.shape[1]):\n",
    "    for j in range(proportion.shape[0]):\n",
    "        plt.text(i, proportion.iloc[:j+1, i].sum() - proportion.iloc[j, i] / 2, f'{proportion.iloc[j, i]*100:.1f}%', ha='center', va='center', \n",
    "                 color='white', fontsize=8)\n",
    "\n",
    "plt.title('Proportion of Sales by Lifestage and Premium Customer Flag')\n",
    "plt.xlabel('Lifestage')\n",
    "plt.ylabel('Proportion of Sales')\n",
    "plt.xticks(rotation=90)\n",
    "plt.legend(title='Premium Customer', bbox_to_anchor=(1.05, 1), loc='upper left')\n",
    "plt.show()"
   ]
  },
