   "source": [
    "avg_units = (QVI_data.groupby(['LIFESTAGE', 'PREMIUM_CUSTOMER'])\n",
    "    .apply(lambda x: x['PROD_QTY'].sum() / x['LYLTY_CARD_NBR'].nunique())\n",
    "    .reset_index(name='AVG')\n",
    "    .sort_values(by='AVG', ascending=False)\n",
    ")\n",
    "avg_units"
   ]
  },
